  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WSydnA/driving-test-alerter.git"
  },
  "dependencies": {
    "casperjs": "1.1.4",
    "express": "4.16.3",
    "slimerjs": "1.0.0"
  }
}
